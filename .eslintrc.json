{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules":{
    "@next/next/no-img-element": "off",
    // "react-hooks/exhaustive-deps": "off",
    "react-hooks/rules-of-hooks": "error",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "parser": "flow",
        "endOfLine": "auto"
      }
    ]
  }
}
